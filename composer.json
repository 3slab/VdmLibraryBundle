{
    "name": "3slab/vdm-library-bundle",
    "description": "Standardize vdm block development",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Jonathan Bouzekri",
            "email": "jonathan.bouzekri@suez.com"
        },
        {
            "name": "Julian Mari√©",
            "email": "julian.marie@free.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vdm\\Bundle\\LibraryBundle\\": ""
        }
    },
    "require": {
        "symfony/console": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/serializer-pack": "*",
        "symfony/yaml": "^5.0",
        "symfony/messenger": "^5.0",
        "symfony/stopwatch": "^5.0",
        "datadog/php-datadogstatsd": "^1.4",
        "doctrine/doctrine-bundle": "^2.0.7",
        "doctrine/mongodb-odm": "^2.0.5",
        "doctrine/mongodb-odm-bundle": "^4.1"
    },
    "suggest": {
        "3slab/vdm-library-http-transport-bundle": "if you want use http as a source",
        "3slab/vdm-library-ftp-transport-bundle": "if you want use ftp as a source",
        "3slab/vdm-library-elasticsearch-transport-bundle": "if you want use elsatcicsearch as a destination"
    },
    "require-dev": {
        "ext-xml": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "phpunit/phpunit": "^9"
    }
}
